{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/location",
      "dest": "api/location.py"
    },
    {
      "src": "/locations",
      "dest": "api/locations.py"
    },
    {
      "src": "/",
      "dest": "templates/index.html"
    }
  ]
}